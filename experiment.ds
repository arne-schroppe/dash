/--

fib n =
  one = 1
  two = 2
  match n begin
    0 -> 0
    1 -> 1
    _ -> (fib (n - one)) + (fib (n - two))
  end

fib 35

--/


rec n sub =
  rec' n' acc =
    match n' begin
      0 -> acc
      _ -> rec' (n' - sub) (acc + n')
    end
  rec' n 0


a = rec 500 1
a
