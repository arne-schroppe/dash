/--

fib n =
  match n begin
    0 -> 0
    1 -> 1
    _ -> (fib (n - 1)) + (fib (n - 2))
  end

fib 35

--/

test-list l =
  match l begin
    []             -> :empty
    [1]            -> :a
    [1, 2]         -> :b
    [a, 3]         -> :c
    [1, 2, 3 | tl] -> :d
  end

test-list [1, 2, 3, 4, 5, 6]



